    <Color x:Key="Color.Brand">#F90606</Color>
    
    <Color x:Key="Color.MainBG">#131212</Color>
    <Color x:Key="Color.BG.Gradient">#211111</Color>
    <Color x:Key="Color.BG.Shape">#1E1E1E</Color>
    
    <Color x:Key="Color.Text.Secondary">#A1A8B5</Color>
    
    <Color x:Key="Color.Stroke">#272727</Color>
    <Color x:Key="Color.Line">#5E6572</Color>
